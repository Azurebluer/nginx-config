######
#
# beta proxy
#
######


server {
  listen 80;
  server_name beta.freecodecamp.org;

  return 301 https://www.freecodecamp.org$request_uri;
}

server {
  listen 443 ssl;
  server_name beta.freecodecamp.org;

  ssl on;
  include snippets/ssl;

  return 301 https://www.freecodecamp.org$request_uri;
}

