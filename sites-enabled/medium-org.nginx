map_hash_bucket_size 256;
map $request_uri $news_uri {
  include /etc/nginx/medium.map;
  default $request_uri;
}

server {
  listen 443 ssl;
  server_name medium.freecodecamp.org medium.freecodecamp.com;

  ssl on;
  include snippets/ssl.conf;
 
  return 301 https://www.freecodecamp.org/news$news_uri;
}

server {
  listen 80;
  server_name blog.freecodecamp.org medium.freecodecamp.org medium.freecodecamp.com;
  return 301 https://medium.freecodecamp.org$request_uri;
}



